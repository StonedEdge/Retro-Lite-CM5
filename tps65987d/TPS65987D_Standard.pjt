# argsList[1] = GPIO vs ADC Mux
def muxSettingUpdateFxn(value, argsList):
    if value == 2:
        argsList[0].setFromInt(1)
        argsList[1].setFromInt(0)
    elif value == 1:
        argsList[0].setFromInt(0)
        argsList[1].setFromInt(1)
    else:
        argsList[0].setFromInt(0)
        argsList[1].setFromInt(0)


